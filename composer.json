{
    "name": "freis/nova-grid-layout",
    "description": "Tool responsible for allowing changing the sizes of fields. Inpired by codenco-dev/nova-grid-system",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Reis",
            "email": "contacto.felipereis@email.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1"
    },
    "autoload": {
        "psr-4": {
            "Freis\\NovaGridLayout\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freis\\NovaGridLayout\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
